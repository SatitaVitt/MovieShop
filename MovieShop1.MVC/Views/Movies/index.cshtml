@model List<MovieShop1.Entities.Movie>

@{
    ViewBag.Title = "index";
}

<h2>Top Revenue Movies</h2>

<table>
    <tr>
        <td>   Id  </td>
        <td>   Title  </td>
        <td>   Revenue  </td>
        <td>   Budget  </td>
    </tr>

    <tbody>
       
        @foreach (var movie in Model)
        {
        <tr>
            <td>@movie.Id</td>
            <td>@movie.Title</td>
            <td>@movie.Revenue</td>
            <td>@movie.Budget</td>
        </tr>
        }
    </tbody>
</table>


