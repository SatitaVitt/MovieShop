@model List<MovieShop1.Entities.Movie>

@{
    ViewBag.Title = "Home Page";
}

<h2>
    Top Revenue Movies
</h2>

<div class="rounded">
    <div class="container-fluid bg-light">
        <div class="row">
            @foreach (var movie in Model)
            {
                <div class="col-6 col-lg-3 col-sm-4 col-xl-2">
                    @Html.Partial("_MovieCard", movie)
                </div>
            }

        </div>
    </div>
</div>